#include <iostream> 
#include <cmath> 
using namespace std; 

void get_lowest_divisors(int n); 
void get_all_divisors(int n);

int main() { 

    int n = 0; 
    cout << "Enter a number and press ENTER: ";     
    cin >> n;     
    cout << n; 
    get_all_divisors(n);     
    cout << endl;     
    return 0; 
}

void get_all_divisors(int n) {
   int i;
   double sqrt_of_n = sqrt(n); 
   for (i = 2; i <= sqrt_of_n; i++) {
       get_lowest_divisors(n / i);
   }
    
    cout << n;
}

